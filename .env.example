# CityPulse Environment Configuration
# Copy to .env and fill in your values
# In CI, these are injected via GitHub Secrets

# Database
POSTGRES_USER=citypulse
POSTGRES_PASSWORD=citypulse
POSTGRES_DB=citypulse

# Backend (required when running the API outside docker-compose)
# Example for local Postgres:
DATABASE_URL=postgresql://citypulse:citypulse@localhost:5432/citypulse

# Backboard AI Integration (keep secret, do not commit real values)
BACKBOARD_API_KEY=your-api-key-here

# Frontend API URL
VITE_API_URL=http://localhost:8000
